      <xs:attribute name="DescEquipo" type="xs:string" />
      <xs:attribute name="FechaVencimiento" type="xs:string" />
      <xs:attribute name="Cantidad" type="xs:int" />
      <xs:attribute name="Precio" type="xs:decimal" />
      <xs:attribute name="Total" type="xs:decimal" />
      <xs:attribute name="DescTipoEntrega" type="xs:string" />
    </xs:complexType>
  </xs:element>


</xs:schema>
