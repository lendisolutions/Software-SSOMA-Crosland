<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="reporteepp"
    targetNamespace="http://tempuri.org/reporteepp.xsd"
    elementFormDefault="qualified"
    xmlns="http://tempuri.org/reporteepp.xsd"
    xmlns:mstns="http://tempuri.org/reporteepp.xsd"
    xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="reporteepp">
    <xs:complexType>
      <xs:sequence />
      <xs:attribute name="RazonSocial" type="xs:string" />
      <xs:attribute name="Logo" type="xs:base64Binary" />
      <xs:attribute name="DescUnidadMinera" type="xs:string" />
      <xs:attribute name="DescArea" type="xs:string" />
      <xs:attribute name="NumeroSolicitud" type="xs:string" />
