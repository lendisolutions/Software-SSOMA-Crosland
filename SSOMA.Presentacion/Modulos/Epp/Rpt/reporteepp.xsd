      <xs:attribute name="SectorResponsable" type="xs:string" />
      <xs:attribute name="Cargo" type="xs:string" />
      <xs:attribute name="Fecha" type="xs:string" />
      <xs:attribute name="Observacion" type="xs:string" />
      <xs:attribute name="Periodo" type="xs:string" />
      <xs:attribute name="Mes" type="xs:string" />
      <xs:attribute name="Item" type="xs:int" />
